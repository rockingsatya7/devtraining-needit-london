        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
	if (isLoading || newValue === '') {
		return;
	}
	
	
	var getEmailScript = new GlideAjax('GetEmailAddress');
	getEmailScript.addParam('sysparm_name','getEmail');
	getEmailScript.addParam('sysparm_user_sysid',g_form.getValue('u_requested_for'));
	getEmailScript.getXML(parseResponse);

	
	function parseResponse(response){
		var answer = response.responseXML.documentElement.getAttribute("answer");
		alert(answer);
		g_form.setValue('u_request_mail',answer);
	}
	
}]]></script>
